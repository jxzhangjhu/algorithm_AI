    "# 就是对进位不了解或者不熟悉\n",
    "num = 123\n",
    "\n",
    "print(num // 10)  # 最高位 + 次高位\n",
    "print(num // 100) # 最高位\n",
    "print(num // 1000) # 0\n",
    "\n",
    "print(num % 10) # 个位\n",
    "print(num % 100) # 十位 + 个位\n",
    "print(num % 1000) # 百分位 + 十位 + 个位\n",
    "\n",
